# game/script.rpy:5193
translate turkish ep4Computer_86e9616c:

    # b "{i}There is no chance this guy alone could beat them both at the same time.. Bob and Mikey were the best fighters in our gang.."
    b "{i}Bu adamın ikisini aynı anda tek başına yenme şansı yok. Bob ve Mikey çetemizdeki en iyi dövüşçülerdi..."

# game/script.rpy:5195
translate turkish ep4Computer_50c4fc75:

    # b "{i}..."
    b  "{i}..."

# game/script.rpy:5196
